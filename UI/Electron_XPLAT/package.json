{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "gypfile": true,
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager ./ --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package": "electron-packager ./ --overwrite --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^12.0.4",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.1"
  },
  "dependencies": {
    "bindings": "*",
    "node-addon-api": "*",
    "ws": "^7.4.5"
  }
}
